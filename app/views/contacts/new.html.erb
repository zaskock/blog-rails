<p>Find me in app/views/contacts/contacts.html.erb</p>
<h1>Contact Us</h1>

<%=form_for :contact, url: contacts_path do |f|%>
	<p>
		<%=f.label :email%></br>
		<%=f.text_field :email%>
	</p>	
	<p>
		<%=f.label :message%></br>
		<%=f.text_area :message%>
	</p>
	<p>
		<%=f.submit%></br>
	</p>
<%end%>

</form>

